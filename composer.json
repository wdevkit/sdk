{
    "name": "wdevkit/sdk",
    "description": "A sdk to wdevkit services.",
    "keywords": [
        "wdevkit",
        "sdk"
    ],
    "homepage": "https://github.com/wdevkit/sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Gilmar Pereira",
            "email": "wdarking@gmail.com",
            "homepage": "https://github.com/wdarking",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "symfony/var-dumper": "^5.1",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Wdevkit\\Sdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wdevkit\\Sdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
